algoritmo "operadoresRelacionais"


var

A, B, C : Inteiro



inicio

      A <- 2
      B <- 3
      C <- 5
      Escreval(A>B)
      Escreval(A=B)
      Escreval(A<>B)
      Escreval(A<B)
      Escreval(A<=B)
      Escreval(A>=B)
      Escreval(C=A+B)
      Escreval(C>=B^A)
      Escreva(B>=C%A)


fimalgoritmo
